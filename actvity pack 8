#divisible number
numerater = int(input("Enter a number:"))
denominater = int(input("Enter another number:"))
if numerater/denominater ==0:
    print("the number is divisible")
else:
    print("the number is not divisible")
#mean val
numbers = [1,3,5,7,11]
mean = sum(numbers)/len(numbers)
print(mean)
mean1 = 38
wrongNumber = 36
correctNumber = 56
totalNum = 40
sum = (mean1 * totalNum)
print (sum)
num2 = (sum - wrongNumber + correctNumber)
mean2 = num2 // totalNum
print(mean2)

#avg spee
a = int(input("input speed of cyclist a"))
b = int(input("input speed of cyclist b"))
c = int(input("input speed of cyclist c"))
avg = a+b+c / 3
print(avg)
if avg > a and avg > b:
    print("average speed is greater than cyclist a and b")
elif avg > a and avg > c:
    print("average speed is greater than cyclist a and c")
elif avg > b and avg > c:
    print("average speed is greater than cyclist b and c")
elif avg > a:
    print("average speed is greater than cyclist a")
elif avg > b:
    print("average speed is greater than cyclist b") 
elif avg > c:
    print("average speed is greater than cyclist c")
else:
    print("how")
